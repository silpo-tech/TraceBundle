{
    "name": "phplib/trace-bundle",
    "type": "symfony-bundle",
    "description": "Trace bundle for Symfony",
    "license": "proprietary",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.3",
        "symfony/http-kernel": "^6.4|^7.0",
        "ramsey/uuid": "^4.7",
        "monolog/monolog": "^3.6",
        "symfony/dependency-injection": "^6.4|^7.0",
        "symfony/config": "^6.4|^7.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "TraceBundle\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/fozzy/"
        },
        {
            "packagist.org": false
        }
    ],
    "suggest": {
        "sentry/sentry": "Adds sentry tracing middleware to Guzzle handlers",
        "phplib/message-bus-bundle": "Adds correlation ids to messages and to logs"
    },
    "conflict": {
        "phplib/message-bus-bundle": "<1.6"
    },
    "prefer-stable": true,
    "require-dev": {
        "sentry/sentry": "^4.9",
        "phplib/message-bus-bundle": "^1.6"
    }
}